public class Queue{
    Node front, rear;
    int length;
    
    Queue(){
        length = 0;
        front = rear = null;
    }
    
    private class Node{
        int data;
        Node next;
        
        Node(int data){
            this.data = data;
            next = null;
        }
    }//eof Node
    
   public boolean isEmpty(){
        return length == 0;
    }
    
    public int length(){
        return length;
    }
    
    public void enque(int data){
        Node temp = new Node(data);
        if(isEmpty()){
            front = temp;
        }
        else{
            rear.next = temp;
        }
        rear = temp;
        length++;
    }
     public void print(){
         if(isEmpty()){
             return;
         } 
         while(front != null){
             System.out.print(front.data+"-->");
             front = front.next;
         }
         System.out.print("null");
     }
    
    public static void main(String[] args){
        Queue queue = new Queue();
        queue.enque(1);
        queue.enque(2);
        queue.enque(3);
        
        queue.print();
    }
}
